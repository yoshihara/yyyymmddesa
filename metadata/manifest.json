{
    "name": "yyyymmddesa",
    "version": "0.1",
    "manifest_version": 2,

    "description": "Tiny tools for daily report in esa.io",

    "permissions": [
        "storage"
    ],

    "options_page": "options.html",

    "content_scripts": [
    {
      "matches": ["https://*.esa.io/posts/*"],
      "js": ["content.bundle.js"]
    }],

    "icons": {
        "16"  : "icon-16.png",
        "48"  : "icon-48.png",
        "128" : "icon-128.png"
    }
}
